<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="organisationalChart.organisationalChart.presentation.OrganisationalChartModelWizardID"
            name="%_UI_OrganisationalChartModelWizard_label"
            class="organisationalChart.organisationalChart.presentation.OrganisationalChartModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/OrganisationalChartModelFile.gif">
         <description>%_UI_OrganisationalChartModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="organisationalChart.organisationalChart.presentation.OrganisationalChartEditorID"
            name="%_UI_OrganisationalChartEditor_label"
            icon="icons/full/obj16/OrganisationalChartModelFile.gif"
            extensions="organisationalchart"
            class="organisationalChart.organisationalChart.presentation.OrganisationalChartEditor"
            contributorClass="organisationalChart.organisationalChart.presentation.OrganisationalChartActionBarContributor">
      </editor>
   </extension>

</plugin>
